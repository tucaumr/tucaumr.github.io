{
  "name": "personal-website",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "build:css": "sass --quiet-deps style/main.scss style/main.css",
    "watch:css": "sass --quiet-deps --watch style/main.scss:style/main.css",
    "serve": "node ./node_modules/live-server/live-server.js --port=5500 --open=index.html",
    "dev": "yarn build:css && concurrently \"yarn watch:css\" \"yarn serve\""
  },
  "repository": "https://github.com/tucaumr/tucaumr.github.io",
  "author": "Arthur Candido Regueira de Miranda <arthur.regueira@aol.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.8",
    "concurrently": "^9.2.1",
    "live-server": "^1.2.2",
    "sass": "^1.97.3"
  }
}
